{
    "name": "carbon",
    "displayName": "carbon",
    "description": "Language support for carbon.",
    "version": "0.2.0",
    "repository": {
        "url": "https://github.com/peterekepeter/carbon"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "scripts": {
		"pack": "npx vsce package"
    },
    "contributes": {
        "languages": [{
            "id": "carbon",
            "aliases": ["carbon", "carbon"],
            "extensions": [".co2"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "carbon",
            "scopeName": "source.carbon",
            "path": "./syntaxes/carbon.tmLanguage.json"
        }]
    }
}